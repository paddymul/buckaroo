    "recharts": "^2.13.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.13.0",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.52.0",
    "@storybook/addon-essentials": "^8.5.6",
    "@storybook/addon-interactions": "^8.5.6",
